@article{Yau2013,
	Abstract = {Recent major cancer genome sequencing studies have used whole-genome
	sequencing to detect various types of genomic variation. However,
	a number of these studies have continued to rely on SNP array information
	to provide additional results for copy number and loss-of-heterozygosity
	estimation and assessing tumour purity. OncoSNP-SEQ is a statistical
	model-based approach for inferring copy number profiles directly
	from high-coverage whole genome sequencing data that is able to account
	for unknown tumour purity and ploidy.MATLAB code is available at
	the following URL: https://sites.google.com/site/oncosnpseq/.Contact:
	c.yau@imperial.ac.uk SUPPLEMENTARY INFORMATION: Supplementary data
	are available at Bioinformatics online.},
	Author = {Christopher Yau},
	Date-Added = {2013-11-08 20:41:00 +0000},
